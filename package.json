{
  "name": "rc-if-else",
  "version": "1.0.1-2",
  "description": "conditional rendering for react components",
  "keywords": [
    "react",
    "react-if-else",
    "if-else",
    "conditional-rendering",
    "show-hide-components"
  ],
  "files": [
    "es",
    "lib"
  ],
  "main": "./lib/index",
  "module": "./es/index",
  "author": "qinyuanbin",
  "maintainers": [
    "a591737386@163.com"
  ],
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "release": "npm run transpile && standard-version && git push --follow-tags origin master && npm publish",
